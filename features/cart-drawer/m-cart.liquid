  .mcart-featured-collection{
    padding-bottom: 20px;
    padding-inline: 28px;
  }
  .mcart-fc-show-details{
    color: var(--Primary, #3D3D3D);
    font-size: 16px;
    font-style: normal;
    font-weight: 500;
    line-height: 20px;
    text-decoration: none;
  }
  .mcart-fc-btn{
    color: #FFF;
    font-size: 14px;
    font-style: normal;
    font-weight: 600;
    line-height: 1;
    border-radius: 2px;
    background: var(--Primary, #135d4b);
    height: 28px;
    width: max-content;
    box-sizing: border-box;
